        "dist/*",
        "dist/*/index.d.ts"
      ]
    }
  },
  "scripts": {
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "build": "NODE_OPTIONS='--max-old-space-size=16384' tsup",
    "dev": "npm run build -- --watch",
    "test:watch": "jest --watch",
    "check:types": "tsc --noEmit",
    "check:package-types": "attw --pack .",
    "check:package-lint": "publint --strict"
