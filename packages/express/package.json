{
  "name": "@frames.js/express",
  "description": "Use frames.js in your Express.js app",
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "index.d.cts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "scripts": {
    "lint": "eslint \"./src/**/*.ts*\"",
    "build": "tsup `find ./src -type f \\( -regex '.*\\.tsx*' -a -not -regex '.*\\.test\\.tsx*' \\)` --format cjs,esm --dts",
    "dev": "npm run build -- --watch",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/framesjs/frames.js/tree/main/packages/express"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "engineStrict": true,
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/supertest": "^6.0.2",
    "@xmtp/frames-validator": "^0.5.1",
    "express": "^4.19.1",
    "supertest": "^6.3.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "license": "MIT",
  "peerDependencies": {
    "@xmtp/frames-validator": "^0.5.1",
    "express": "^4.19.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@frames.js/core": "^0.0.0"
  }
}
